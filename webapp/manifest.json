{
    "_version": "1.37.0",
    "sap.app":{
        "id":"emc.fin.ar",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "i18n": "i18n/i18n.properties",
        "dataSources": {
            "anubhavService": {
                "type": "OData",
                "uri": "/sap/opu/odata/sap/ZMY_ODATA_SRV_01/",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "deviceTypes": {
            "desktop": true,
            "phone": true,
            "tablet": true
        },
        "supportedThemes": ["sap_belize","sap_fiori_3_dark"]
    },
    "sap.ui5":{
        "library": {
            "css": "css/mystyle.css"
        },
        "rootView":{
            "viewName": "emc.fin.ar.view.App",
            "type": "XML",
            "id": "idAppView"
        },
        "routing": {
            "config":{
                "viewType": "XML",
                "viewPath": "emc.fin.ar.view",
                "controlId": "appCon",
                "clearControlAggregation": true
            },
            "routes":[{
                "name": "spiderman",
                "pattern": "",
                "target":["joe", "biden"]
            },{
                "name": "superman",
                "pattern": "product/{pid}",
                "target":["joe", "modi"]
            },{
                "name": "add",
                "pattern": "addProduct",
                "target":["joe", "putin"]
            }],
            "targets": {
                "joe":{
                    "viewName": "View1",
                    "id": "idView1",
                    "controlAggregation": "masterPages"
                },
                "biden":{
                    "viewName": "Empty",
                    "id": "idEmpty",
                    "controlAggregation": "detailPages"
                },
                "modi":{
                    "viewName": "View2",
                    "id": "idView2",
                    "controlAggregation": "detailPages"
                },
                "putin":{
                    "viewName": "Add",
                    "id": "idAdd",
                    "controlAggregation": "detailPages"
                }
            }
        },
        "dependencies": {
            "minUI5Version": "1.90.0",
            "libs": {
                "sap.m": {
                    "minVersion": "1.90.0"
                },
                "sap.ui.layout": {
                    "minVersion": "1.90.0"
                }
            }
        },
        "models": {
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "dataSource": "anubhavService"
            },
            "client": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "mockdata/fruits.json"
            },
            "i18n":{
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {"bundleUrl": "i18n/i18n.properties"}
            }
        }
    }
}